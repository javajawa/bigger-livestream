{
  "name": "Bigger Livestream",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Makes the livestream video box 720p (HD) sized",
  "content_scripts": [{
    "css": [ "bigger.css" ],
	"js":  [ "fix.js" ],
    "matches": [ "http://new.livestream.com/*" ],
    "all_frames": true
  }],
  "offline_enabled": true,
  "icons" : { "128" : "icon_128.png"  }
}